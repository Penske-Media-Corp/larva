<section class="lrv-a-wrapper lrv-u-padding-t-1" data-slider>

	{% include "@larva/components/c-heading/c-heading.twig" with c_heading %}

	<div class="latest-videos // lrv-u-overflow-hidden lrv-a-glue-parent">
		<ul class="lrv-a-unstyle-list lrv-a-scrollable-grid@desktop-max" data-slider-track>
			{% for item in latest_videos %}
				<li data-slider-item>
					{% include "@larva/modules/vlanding-video-card/vlanding-video-card.twig" with item %}
				</li>
			{% endfor %}
		</ul>
	</div>
</section>
