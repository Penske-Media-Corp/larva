{
  "name": "@penskemediacorp/larva",
  "version": "1.0.0",
  "scripts": {
    "test": "./node_modules/.bin/jest --forceExit",
    "larva": "packages/larva/bin/larva.js server --watch ../larva-patterns --watch src -e twig,js,html",
    "backstop": "backstop --config=node_modules/@penskemediacorp/backstopjs-config",
    "prod": "npm run prod:scss && npm run prod:js",
    "prod:scss": "packages/larva/bin/larva.js prod-scss",
    "prod:js": "packages/larva/bin/larva.js prod-js",
    "dev": "concurrently \"npm:dev:scss\" \"npm:dev:js\" --raw",
    "dev:scss": "packages/larva/bin/larva.js dev-scss",
    "dev:js": "packages/larva/bin/larva.js dev-js",
    "lint": "packages/larva/bin/larva.js lint-scss & larva lint-js",
    "lint:scss": "packages/larva/bin/larva.js lint-scss",
    "lint:js": "packages/larva/bin/larva.js lint-js ./",
    "lint-fix": "packages/larva/bin/larva.js lint-scss --fix && npm run lint:js --fix",
    "lint-fix:scss": "packages/larva/bin/larva.js lint-scss --fix",
    "lint-fix:js": "packages/larva/bin/larva.js lint-js --fix",
    "write-json": "packages/larva/bin/larva.js write-json larva && packages/larva/bin/larva.js write-json",
    "build-icons": "packages/larva/bin/larva.js build-icons",
    "build-html": "packages/larva/bin/larva.js build-html larva && packages/larva/bin/larva.js build-html project",
    "build-tokens": "rm -r build/tokens && mkdir -p build/tokens && cp packages/larva-tokens/build/* build/tokens"
  },
  "bin": {
    "larva": "packages/larva/bin/larva.js"
  },
  "dependencies": {
    "@babel/core": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@penskemediacorp/backstopjs-config": "^0.1.10",
    "@penskemediacorp/eslint-config": "^0.1.10",
    "@penskemediacorp/larva-css": "^0.1.10",
    "@penskemediacorp/larva-js": "^0.1.11",
    "@penskemediacorp/larva-patterns": "^0.1.11",
    "@penskemediacorp/larva-scss": "^0.1.10",
    "@penskemediacorp/larva-svg": "^0.1.2",
    "@penskemediacorp/larva-tokens": "^0.1.10",
    "@penskemediacorp/stylelint-config": "^0.1.2",
    "@penskemediacorp/twig-to-php-parser": "^0.1.10",
    "axios": "^0.21.1",
    "babel-loader": "^8.1.0",
    "chalk": "^4.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.2.0",
    "cross-spawn": "^7.0.3",
    "cssnano": "^4.1.10",
    "eslint": "^7.4.0",
    "eslint-loader": "^4.0.2",
    "express": "^4.17.1",
    "flickity": "^2.2.1",
    "fs-extra": "^9.0.1",
    "globby": "^11.0.1",
    "gulp": "^4.0.2",
    "gulp-clean": "^0.4.0",
    "gulp-cli": "^2.3.0",
    "gulp-postcss": "^8.0.0",
    "gulp-sass": "^4.1.0",
    "gulp-stylelint": "^13.0.0",
    "lodash.clonedeep": "^4.5.0",
    "marked": "^1.1.1",
    "node-sass-glob-importer": "^5.3.2",
    "nodemon": "^2.0.4",
    "stylelint": "^13.6.1",
    "svg-sprite": "^1.5.0",
    "svgo": "^1.3.2",
    "through2": "^4.0.2",
    "twing": "^5.0.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  },
  "devDependencies": {
    "@babel/core": "^7.12.7",
    "@babel/preset-env": "^7.12.7",
    "backstopjs": "^4.5.1",
    "bluebird": "^3.7.2",
    "cssnano": "^4.1.10",
    "express": "^4.17.1",
    "gaze": "^1.1.3",
    "globby": "^10.0.2",
    "gulp": "^4.0.2",
    "gulp-clean": "^0.4.0",
    "gulp-concat": "^2.6.1",
    "gulp-postcss": "^8.0.0",
    "gulp-rename": "^1.4.0",
    "gulp-sass": "^4.1.0",
    "gulp-stylelint": "^9.0.0",
    "jest": "^24.8.0",
    "lerna": "^3.22.1",
    "node-sass": "^4.14.1",
    "sass-true": "^5.0.0",
    "stylelint": "^10.1.0",
    "twing": "^2.3.7",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-merge": "^4.2.2"
  }
}
